  "private": true,
  "scripts": {
    "android": "./node_modules/react-native/cli.js run-android",
    "ios": "react-native run-ios",
    "start": "./node_modules/react-native/cli.js start",
    "test": "jest",
    "lint": "eslint . --fix",
